<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>WebApiProxy.CSharp</id>
    <title>$title$</title>
    <projectUrl>https://github.com/onionhammer/WebApiProxy</projectUrl>
    <version>$version$</version>
    <authors>Fanie Reynders</authors>
    <owners>Fanie Reynders</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Generates a C# proxy for communicating to a RESTful Web Api that implements the WebApiProxy provider</description>
    <releaseNotes></releaseNotes>
    <copyright>$copyright$</copyright>
    <tags>AspNet WebAPI Proxy JavaScript CSharp</tags>
    <dependencies>
      <dependency id="Microsoft.AspNet.WebApi.Client" version="5.2.3" />
    </dependencies>
    <contentFiles>
      <files include="WebApiProxy/*.config" buildAction="None" />
    </contentFiles>
  </metadata>
  <files>
    <file src="WebApiProxy\WebApiProxy.config" target="content\WebApiProxy\WebApiProxy.config" />
    <file src="WebApiProxy\WebApiProxy.generated.cs" target="content\WebApiProxy\WebApiProxy.generated.cs" />
    
    <file src="init.ps1" target="tools" />
    <file src="WebApiProxyCSharp.psm1" target="tools" />
    <file src="bin\Debug\*.dll" target="tools" />
    <file src="bin\Debug\*.targets" target="tools" />
  </files>
</package>